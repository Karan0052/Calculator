a = int(input("Enter your first number: "))
b = int(input("Enter second number: "))
sub = a-b

if(sub < 0):
  print ("The value of ", a , "-" , b , "is :", sub * -1)
else:

 print("The value of ", a , "-" , b , "is :", sub)

print ("The value of ", a , "+" , b , "is :", a + b)
print ("The value of ", a , "*" , b , "is :", a * b)
print ("The value of ", a , "/" , b , "is :", a / b)
